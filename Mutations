function mutation(arr) {
  var first = arr[0].toLowerCase();
  var second = arr[1].toLowerCase();
  var response = 0;
  
  for(var i=0;i<=second.length;i++){
    if (response==-1){
      return false;
    }
    response = first.indexOf(second.charAt(i));
  }
  return true;
}

mutation(["hello", "hey"]);
